{
  "name": "sqlserver-connector",
  "config": {
    "connector.class": "io.debezium.connector.sqlserver.SqlServerConnector",
    "tasks.max": "1",
    "database.hostname": "host.docker.internal",
    "database.port": "1433",
    "database.user": "${env:SQLSERVER_USER}",
    "database.password": "${env:SQLSERVER_PASS}",
    "database.names": "AdventureWorks2022",
    "topic.prefix": "mssql",
    "database.encrypt": "false",
    "snapshot.mode": "initial",
    "schema.history.internal.kafka.bootstrap.servers": "kafka1:29092,kafka2:29092,kafka3:29092",
    "schema.history.internal.kafka.topic": "schema-changes.mssql",
    "key.converter": "org.apache.kafka.connect.storage.StringConverter",
    "value.converter": "io.confluent.connect.avro.AvroConverter",
    "value.converter.schema.registry.url": "http://schema-registry:8081"
  }
}
